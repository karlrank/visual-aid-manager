{
  "name": "visual-aid-manager",
  "productName": "Visual Aid Manager",
  "description": "A manager for Visual aid material",
  "version": "0.0.1",
  "author": "Karl Rankla",
  "copyright": "Â© 2017, Karl Rankla",
  "homepage": "https://github.com/karlrank/visual-aid-manager",
  "license": "UNLICENSED",
  "main": "app/background.js",
  "build": {
    "appId": "karlrank.visual-aid-manager",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "electron-log": "1.3.0",
    "fs-jetpack": "^0.10.2",
    "handlebars": "4.0.6",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-define-module": "^0.1.5",
    "gulp-handlebars": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "3.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "gulp-wrap": "^0.13.0",
    "istanbul": "^0.4.3",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "run-sequence": "^1.2.2",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
}
